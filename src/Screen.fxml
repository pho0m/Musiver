<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
  <children>
    <MenuBar VBox.vgrow="NEVER" />
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="23.0" layoutY="7.0" prefHeight="70.0" prefWidth="576.0" text="Mp3 player">
               <font>
                  <Font size="53.0" />
               </font>
            </Label>
            <ProgressBar layoutX="28.0" layoutY="90.0" prefHeight="18.0" prefWidth="584.0" progress="0.0" />
            <Button fx:id="playButton" onAction="#play" layoutX="28.0" layoutY="133.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="63.0" text="Play" />
            <Button fx:id="pauseButton" onAction="#pause" layoutX="112.0" layoutY="133.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="63.0" text="Pause" />
            <Button fx:id="stopButton" onAction="#stop" layoutX="193.0" layoutY="133.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="63.0" text="Stop" />
            <Button fx:id="previousButton" onAction="#previous" layoutX="279.0" layoutY="133.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="63.0" text="Previous" />
            <Button fx:id="nextButton" onAction="#next" layoutX="365.0" layoutY="133.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="63.0" text="Next" />
            <Slider layoutX="28.0" layoutY="221.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
